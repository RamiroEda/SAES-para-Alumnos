<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/parent"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:clipToPadding="false"
    android:nestedScrollingEnabled="true"
    android:paddingBottom="58dp"
    tools:ignore="HardcodedText">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="16dp">

        <TextView

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:text="Generales"
            android:textColor="@color/colorTextSecondary"
            android:textSize="24sp"
            android:textStyle="bold" />

        <TextView

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Peso"
            android:textColor="@color/colorTextSecondary"
            android:textSize="14sp" />

        <TextView

            android:id="@+id/weigth_text_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="..."
            android:textColor="@color/colorTextPrimary"
            android:textSize="18sp" />

        <TextView

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="Estatura"
            android:textColor="@color/colorTextSecondary"
            android:textSize="14sp" />

        <TextView

            android:id="@+id/height_text_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="..."
            android:textColor="@color/colorTextPrimary"
            android:textSize="18sp" />

        <TextView

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="Tipo de sangre"
            android:textColor="@color/colorTextSecondary"
            android:textSize="14sp" />

        <TextView

            android:id="@+id/blood_type_text_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="..."
            android:textColor="@color/colorTextPrimary"
            android:textSize="18sp" />

        <TextView

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            android:layout_marginBottom="8dp"
            android:text="Afiliación Médica"
            android:textColor="@color/colorTextSecondary"
            android:textSize="24sp"
            android:textStyle="bold" />

        <TextView

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Institución"
            android:textColor="@color/colorTextSecondary"
            android:textSize="14sp" />

        <TextView

            android:id="@+id/institution_text_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="..."
            android:textColor="@color/colorTextPrimary"
            android:textSize="18sp" />

        <TextView

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="No. de seguro social"
            android:textColor="@color/colorTextSecondary"
            android:textSize="14sp" />

        <TextView

            android:id="@+id/social_security_number_text_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="..."
            android:textColor="@color/colorTextPrimary"
            android:textSize="18sp" />

        <TextView

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="Fecha de alta "
            android:textColor="@color/colorTextSecondary"
            android:textSize="14sp" />

        <TextView

            android:id="@+id/register_date_text_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="..."
            android:textColor="@color/colorTextPrimary"
            android:textSize="18sp" />

        <TextView

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="Fecha de baja "
            android:textColor="@color/colorTextSecondary"
            android:textSize="14sp" />

        <TextView

            android:id="@+id/deregister_date_text_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="..."
            android:textColor="@color/colorTextPrimary"
            android:textSize="18sp" />

        <TextView

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            android:layout_marginBottom="8dp"
            android:text="Integridad Física"
            android:textColor="@color/colorTextSecondary"
            android:textSize="24sp"
            android:textStyle="bold" />

        <TextView

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Padecimiento de enfermedades"
            android:textColor="@color/colorTextSecondary"
            android:textSize="14sp" />

        <TextView

            android:id="@+id/diseases_text_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="..."
            android:textColor="@color/colorTextPrimary"
            android:textSize="18sp" />

        <TextView

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="Problemas físicos"
            android:textColor="@color/colorTextSecondary"
            android:textSize="14sp" />

        <TextView

            android:id="@+id/physical_problems_text_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="..."
            android:textColor="@color/colorTextPrimary"
            android:textSize="18sp" />

        <TextView

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="Tatuado"
            android:textColor="@color/colorTextSecondary"
            android:textSize="14sp" />

        <TextView

            android:id="@+id/tattoos_text_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="..."
            android:textColor="@color/colorTextPrimary"
            android:textSize="18sp" />

        <TextView

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="Pie plano"
            android:textColor="@color/colorTextSecondary"
            android:textSize="14sp" />

        <TextView

            android:id="@+id/flatfoot_text_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="..."
            android:textColor="@color/colorTextPrimary"
            android:textSize="18sp" />

    </LinearLayout>
</ScrollView>